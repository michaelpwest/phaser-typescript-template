{
  "name": "phaser-typescript-template",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "generate-logo:android": "npx @capacitor/assets generate --android",
    "generate-logo:ios": "npx @capacitor/assets generate --ios",
    "deploy:android": "npm run generate-logo:android && npm run build && cap run android",
    "deploy:ios": "npm run generate-logo:ios && npm run build && cap run ios",
    "eslint": "eslint .",
    "prettier": "prettier --config .prettierrc --check --write ."
  },
  "author": "My Name",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@capacitor/android": "7.3.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/cli": "7.3.0",
    "@capacitor/core": "7.3.0",
    "@capacitor/ios": "7.3.0",
    "@capacitor/preferences": "7.0.1",
    "@capacitor/screen-orientation": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@types/lodash": "4.17.17",
    "@types/node": "24.0.1",
    "es6-promise-plugin": "4.2.2",
    "i18n-for-browser": "3.0.0",
    "lodash": "4.17.21",
    "phaser": "4.0.0-rc.4",
    "typescript": "5.8.3",
    "vite": "6.3.5"
  },
  "devDependencies": {
    "@capacitor/assets": "3.0.5",
    "@eslint/js": "9.29.0",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.1",
    "globals": "16.2.0",
    "lint-staged": "16.1.2",
    "prettier": "3.5.3",
    "typescript-eslint": "8.34.0",
    "yorkie": "2.0.0"
  }
}
