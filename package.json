{
  "name": "phaser-typescript-template",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "generate-logo:android": "npx @capacitor/assets generate --android",
    "generate-logo:ios": "npx @capacitor/assets generate --ios",
    "deploy:android": "npm run generate-logo:android && npm run build && cap run android",
    "deploy:ios": "npm run generate-logo:ios && npm run build && cap run ios",
    "eslint": "eslint .",
    "prettier": "prettier --config .prettierrc --check --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@capacitor/android": "7.4.2",
    "@capacitor/app": "7.0.1",
    "@capacitor/cli": "7.4.2",
    "@capacitor/core": "7.4.2",
    "@capacitor/ios": "7.4.2",
    "@capacitor/preferences": "7.0.1",
    "es6-promise-plugin": "4.2.2",
    "i18n-for-browser": "3.0.0",
    "phaser": "4.0.0-rc.4",
    "phaser3-rex-plugins": "1.80.16",
    "typescript": "5.9.2",
    "vite": "7.0.6"
  },
  "devDependencies": {
    "@capacitor/assets": "3.0.5",
    "@eslint/js": "9.32.0",
    "@types/node": "24.1.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.3",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.4",
    "prettier": "3.6.2",
    "typescript-eslint": "8.38.0"
  }
}
